<div class="search-box-search">
  <%= form_with(url: tweets_timeline_path, method: :get, local: true) do %>
    <%= text_field :keyword, nil,  id: "tweet_search_timeline", name: "keyword", placeholder: "Account ID for search", style: "width: 200px;"%><button title="検索" type="submit">User Search</button>
  <% end %>
</div>

<div class='tweet_list'>
   <%= render partial: 'tweet_list', locals: {tweets: @timeline}  %>
</div>